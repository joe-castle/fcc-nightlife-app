{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React and Redux Starter Project",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node build/server.js",
    "serv": "nodemon --exec babel-node src/server.js",
    "dev": "NODE_ENV=development node webpack.devserver.js",
    "clean": "rimraf build/",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' src test",
    "test": "NODE_ENV=test mocha src/**/*.test.js --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "check:src": "npm run lint && npm run test",
    "build:commonjs": "NODE_ENV=production babel src --out-dir build",
    "build:min": "NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "build": "npm run build:commonjs && npm run build:min",
    "predeploy": "npm run clean && npm run check:src && npm run build"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.4.0",
    "body-parser": "^1.15.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.4",
    "express-session": "^1.14.2",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.9",
    "redis": "^2.6.1",
    "shortid": "^2.2.6",
    "yelp": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^4.2.0",
    "css-loader": "^3.1.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "mocha": "^6.2.0",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^3.0.0",
    "react-addons-test-utils": "^15.1.0",
    "rupture": "^0.7.1",
    "skin-deep": "^1.2.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "supertest": "^4.0.2",
    "typographic": "^3.0.0",
    "webpack": "^4.38.0",
    "webpack-dev-server": "^3.7.2"
  }
}
